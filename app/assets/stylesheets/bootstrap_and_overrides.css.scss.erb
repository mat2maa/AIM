@import "bootstrap";

html {
}

body {
  background-color: #333333;
  background: image-url('bg.png') no-repeat center center fixed;
  -webkit-background-size: cover;
  -moz-background-size: cover;
  -o-background-size: cover;
  background-size: cover;
  padding-top: 160px;
  padding-bottom: 80px;
  color: #FFFFFF;
  font-family: "OpenSansLight", verdana, arial, helvetica, sans-serif;
  overflow-y: scroll;
  overflow-x: hidden;
}

body.pages.home {
  background: white;
  color: #333;
}

.pages.home .home-video {
  position: absolute;
  width: 50%;
  height: 50%;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
  -moz-transform: translate(-50%, -50%);
  -ms-transform: translate(-50%, -50%);
  -o-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
}

.pages.home .home-video video {
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
  -moz-transform: translate(-50%, -50%);
  -ms-transform: translate(-50%, -50%);
  -o-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  min-width: 100%;
  min-height: 100%;
  width: auto;
  height: auto;
  z-index: -1000;
  overflow: hidden;
  opacity: 0;
}

.pages.home .home-video video.in-view {
  opacity: 1;
}

.pages.home .home-video .home-contact {
  position: absolute;
  margin-top: -125px;
  top: 100%;
  left: 0;
  -webkit-transform: translate(0, -50%);
  -moz-transform: translate(0, -50%);
  -ms-transform: translate(0, -50%);
  -o-transform: translate(0, -50%);
  transform: translate(0, -50%);
  width: 100%;
  height: auto;
  z-index: 1;
  overflow: hidden;
  text-align: center;
}

.pages.home .home-video  .home-contact a {
  -webkit-transition: opacity 0.5s 0.35s ease-in;
  -moz-transition: opacity 0.5s 0.35s ease-in;
  -ms-transition: opacity 0.5s 0.35s ease-in;
  -o-transition: opacity 0.5s 0.35s ease-in;
  transition: opacity 0.5s 0.35s ease-in;
}

.pages.home .home-video  .home-contact h3 {
  -webkit-transition: opacity 0.5s 0s ease-in;
  -moz-transition: opacity 0.5s 0s ease-in;
  -ms-transition: opacity 0.5s 0s ease-in;
  -o-transition: opacity 0.5s 0s ease-in;
  transition: opacity 0.5s 0s ease-in;
}

.pages.home .home-video  .home-contact a,
.pages.home .home-video  .home-contact h3 {
  opacity: 0;
  color: #b2b2b2;
}

.pages.home .home-video  .home-contact a.in-view,
.pages.home .home-video  .home-contact h3.in-view {
  opacity: 1;
}

.pages.home .home-video  .home-contact a:hover {
  color: #999999;
}


