/* Logo list */
#logo-list {
  width: 250px;
  height: 100px;
  position: relative;
  margin: 40px 80px;
}

#logo-list > li {
  width: 250px;
  position: absolute;
  top: 0;
  opacity: 0;
  -webkit-transition: opacity 0.5s;
  -moz-transition: opacity 0.5s;
  -o-transition: opacity 0.5s;
  -ms-transition: opacity 0.5s;
  transition: opacity 0.5s;
}

#logo-list > li.in-view {
  left: 0px;
  opacity: 1;
  -webkit-transition: opacity 0.5s 0.25s, left 0.5s;
  -moz-transition: opacity 0.5s 0.25s, left 0.5s;
  -o-transition: opacity 0.5s 0.25s, left 0.5s;
  -ms-transition: opacity 0.5s 0.25s, left 0.5s;
  transition: opacity 0.5s 0.25s, left 0.5s;
}

#logo-list > li.in-view-plus {
  left: 250px;
  opacity: 0;
  -webkit-transition: opacity 0.5s, left 0.5s;
  -moz-transition: opacity 0.5s, left 0.5s;
  -o-transition: opacity 0.5s, left 0.5s;
  -ms-transition: opacity 0.5s, left 0.5s;
  transition: opacity 0.5s, left 0.5s;
}

#logo-list > li.in-view-minus {
  left: -250px;
  opacity: 0;
  -webkit-transition: opacity 0.5s, left 0.5s;
  -moz-transition: opacity 0.5s, left 0.5s;
  -o-transition: opacity 0.5s, left 0.5s;
  -ms-transition: opacity 0.5s, left 0.5s;
  transition: opacity 0.5s, left 0.5s;
}

#logo-list > .next {
  position:absolute;
  top:-8px;
  right:-80px;
  z-index: 1000;
  cursor: pointer;
}

#logo-list > .prev {
  position:absolute;
  top:-8px;
  left:-80px;
  z-index: 1000;
  cursor: pointer;
}

