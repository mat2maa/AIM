/* Storyboard list */
#storyboard-list {
  width: 782px;
  height: 240px;
  position: relative;
  overflow: hidden;
}

#storyboard-list > li {
  width: 391px;
  position: absolute;
  top: 0;
  opacity: 0;
  -webkit-transition: opacity 0.5s;
  -moz-transition: opacity 0.5s;
  -o-transition: opacity 0.5s;
  -ms-transition: opacity 0.5s;
  transition: opacity 0.5s;
}

#storyboard-list > li.in-view {
  left: 0px;
  opacity: 1;
  -webkit-transition: opacity 0.5s 0.25s, left 0.5s;
  -moz-transition: opacity 0.5s 0.25s, left 0.5s;
  -o-transition: opacity 0.5s 0.25s, left 0.5s;
  -ms-transition: opacity 0.5s 0.25s, left 0.5s;
  transition: opacity 0.5s 0.25s, left 0.5s;
}

#storyboard-list > li.in-view-minus {
  left: -401px;
  opacity: 0;
  -webkit-transition: opacity 0.5s, left 0.5s;
  -moz-transition: opacity 0.5s, left 0.5s;
  -o-transition: opacity 0.5s, left 0.5s;
  -ms-transition: opacity 0.5s, left 0.5s;
  transition: opacity 0.5s, left 0.5s;
}

#storyboard-list > li.in-view-plus {
  left: 401px;
  opacity: 1;
  -webkit-transition: opacity 0.5s, left 0.5s;
  -moz-transition: opacity 0.5s, left 0.5s;
  -o-transition: opacity 0.5s, left 0.5s;
  -ms-transition: opacity 0.5s, left 0.5s;
  transition: opacity 0.5s, left 0.5s;
}

#storyboard-list > li.in-view-plus2 {
  left: 802px;
  opacity: 0;
  -webkit-transition: opacity 0.5s, left 0.5s;
  -moz-transition: opacity 0.5s, left 0.5s;
  -o-transition: opacity 0.5s, left 0.5s;
  -ms-transition: opacity 0.5s, left 0.5s;
  transition: opacity 0.5s, left 0.5s;
}

#scene-info {
  position:relative;
  z-index:2;
}

#scene-images {
  position:relative;
  z-index:1;
}

.video {
  position:fixed;
  top:69px;
  left:0;
  width:100%;
  height:100%;
  z-index: 0;
}

.video > video {
  position: absolute;
  bottom: 0px;
  left: 0px;
  min-width: 100%;
  min-height: 100%;
  width: auto;
  height: auto;
  overflow: hidden;
  opacity: 0;
  z-index: 0;
}

.video > video.in-view {
  opacity: 1.0;
  z-index: 1;
}

/* Helper */
#helper {
  bottom: 100px;
  color: black;
}

/* Progress */
.step-counter {
  position:absolute;
  width:100%;
  margin-top:-15px;
}

.step-counter > p {
  position:absolute;
  font-size: 10px;
  line-height: 12px;
  padding:0;
  margin:0 0 5px 10px;
  text-transform: uppercase;
  -webkit-transition: all 0.2s ease-out;
  -moz-transition: all 0.2s ease-out;
  -ms-transition: all 0.2s ease-out;
  -o-transition: all 0.2s ease-out;
  transition: all 0.2s ease-out;
}

.progress-indicator {
  width: 100%;
  height: 5px;
}

.progress-indicator-background {
  position: absolute;
  height: 5px;
  width: 25%;
}

.progress-indicator-inner {
  position: absolute;
  height: 5px;
  -webkit-transition: all 0.2s ease-out;
  -moz-transition: all 0.2s ease-out;
  -ms-transition: all 0.2s ease-out;
  -o-transition: all 0.2s ease-out;
  transition: all 0.2s ease-out;
}

.progress-indicator-background-25 {
  left: 0%;
  background-color: rgba(186, 237, 109, 0.25);
}

.progress-indicator-background-50 {
  left: 25%;
  background-color: rgba(249, 255, 8, 0.25);
}

.progress-indicator-background-75 {
  left: 50%;
  background-color: rgba(255, 161, 36, 0.25);
}

.progress-indicator-background-100 {
  left: 75%;
  background-color: rgba(236, 28, 36, 0.25);
}

.progress-indicator-inner-25 {
  background-color: rgb(186, 237, 109);
}

.progress-indicator-inner-50 {
  background-color: rgb(249, 255, 8);
}

.progress-indicator-inner-75 {
  background-color: rgb(255, 161, 36);
}

.progress-indicator-inner-100 {
  background-color: rgb(236, 28, 36);
}

/* Side Nav */
#wrap {
  position: absolute;
  left: -460px;
  margin: 0;

  background-color: #1b1b1b;
  background-image: -moz-linear-gradient(top, #303030, #1a1a1a);
  background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#303030), to(#1a1a1a));
  background-image: -webkit-linear-gradient(top, #303030, #1a1a1a);
  background-image: -o-linear-gradient(top, #303030, #1a1a1a);
  background-image: linear-gradient(to bottom, #303030, #1a1a1a);
  background-repeat: repeat-x;
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#FF303030', endColorstr='#FF1a1a1a', GradientType=0);

  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  padding:10px;

  -webkit-border-radius: 0px 15px 15px 0px;
  -moz-border-radius:  0px 15px 15px 0px;
  border-radius:  0px 15px 15px 0px;

  -webkit-transition: all 0.2s ease-out;
  -moz-transition: all 0.2s ease-out;
  -ms-transition: all 0.2s ease-out;
  -o-transition: all 0.2s ease-out;
  transition: all 0.2s ease-out;
}
#wrap.active {
  left: 0px;

  -webkit-box-shadow: 3px 0px 20px rgba(0, 0, 0, 0.4);
  -moz-box-shadow: 3px 0px 20px rgba(0, 0, 0, 0.4);
  box-shadow: 3px 0px 20px rgba(0, 0, 0, 0.4);
}

#side-menu {
  margin: 0;
  padding: 0;
}

a.side-menu-link {
  float: left;
  display: block;
  padding: 1em;
}

/* Page Styles */
.manufacturer-container {
  display:none;
}
.airbus-container {
  display:none;
}
.embraer-container {
  display:none;
}

